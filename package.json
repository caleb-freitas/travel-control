{
  "name": "typescript-setup",
  "version": "1.0.0",
  "description": "typescript project setup",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "start:dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn -r tsconfig-paths/register src/main/server.ts",
    "test": "jest --passWithNoTests --noStackTrace --coverage=false --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest.unit.config.ts",
    "test:e2e": "npm test -- --watch -c jest.e2e.config.ts",
    "test:integration": "npm test -- --watch -c jest.integration.config.ts",
    "test:staged": "npm test",
    "test:ci": "npm test -- --coverage",
    "test:clear": "jest --clearCache",
    "prepare": "husky install",
    "migrate:dev": "npx prisma migrate dev --name postgres-dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-test",
    "test:prisma": "dotenv -e .env.test jest -i",
    "build": "rimraf dist && prisma generate && tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "lint-staged": {
    "*.ts": [
      "eslint 'src/**' --fix",
      "npm run test:staged"
    ],
    "*.js": "eslint --cache --fix"
  },
  "keywords": [],
  "author": "caleb-freitas",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.6",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@ngneat/falso": "^3.1.0",
    "@prisma/client": "^3.10.0",
    "@types/supertest": "^2.0.11",
    "bcrypt": "^5.0.1",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "password-validator": "^5.2.1",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  }
}